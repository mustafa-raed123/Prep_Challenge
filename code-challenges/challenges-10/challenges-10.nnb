                "function compareTriplets(a, b) {\n    let alicePoint =0;\n    let bobPoint = 0;\n    for(let i in a){\n   a[i] == b[i] ? \"\" : a[i] > b[i] ? alicePoint++ : bobPoint++;\n    }\n    let result = [];\n    result.push(alicePoint);\n    result.push(bobPoint);\n    return result\n      \n\n}"
