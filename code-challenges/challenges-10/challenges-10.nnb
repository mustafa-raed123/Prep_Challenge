            "source": [
                "function birthdayCakeCandles(candles) {\n    let tallest =(Math.max(...candles));\n    let result = 0;\n    candles.forEach(element => {\n      if(element == tallest){\n        result ++;\n      }   \n    });\n    return result    \n  \n  }"
